                        android:layout_width="0dp"
                        android:layout_height="28dp"
                        android:layout_weight="1"
                        android:background="@drawable/button_border"
                        android:gravity="center_vertical"
                        android:paddingLeft="@dimen/index_layout_marginLeft"
                        android:text="@string/gmfm_total_score_e"
                        android:textColor="@color/white"
                        android:textSize="@dimen/text_size_15" />

                    <TextView
                        android:id="@+id/total_scores_e_tv"
                        android:layout_width="0dp"
                        android:layout_height="28dp"
                        android:layout_weight="1"
                        android:background="@drawable/sousuo_et_bg"
                        android:gravity="center_vertical"
                        android:inputType="textCapSentences"
                        android:paddingLeft="5dp"
                        android:singleLine="true"
                        android:textColor="@color/gray"
                        android:textSize="@dimen/text_size_15" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/marginTop"
                    android:layout_marginTop="@dimen/marginTop"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <Button
                        android:id="@+id/total_scores_all_bt"
                        android:layout_width="0dp"
                        android:layout_height="28dp"
                        android:layout_weight="1"
                        android:background="@drawable/button_border"
                        android:gravity="center_vertical"
                        android:paddingLeft="@dimen/index_layout_marginLeft"
                        android:text="@string/gmfm_total_score_all"
                        android:textColor="@color/white"
                        android:textSize="@dimen/text_size_15" />

                    <TextView
                        android:id="@+id/total_scores_all_tv"
                        android:layout_width="0dp"
                        android:layout_height="28dp"
                        android:layout_weight="1"
                        android:background="@drawable/sousuo_et_bg"
                        android:gravity="center_vertical"
                        android:inputType="textCapSentences"
                        android:paddingLeft="5dp"
                        android:singleLine="true"
                        android:textColor="@color/gray"
                        android:textSize="@dimen/text_size_15" />
                </LinearLayout>


            </LinearLayout>

        </ScrollView>

        <me.zhangls.rilintech.view.googleprogressbar.GoogleProgressBar
            android:id="@+id/google_progress"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_gravity="center"
            gpb:type="folding_circles" />
    </FrameLayout>

</LinearLayout>
