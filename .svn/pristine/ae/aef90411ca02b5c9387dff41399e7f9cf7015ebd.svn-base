                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:width="150dp"
                            android:text="@string/fim_total_score"
                            android:textColor="@color/gray"
                            android:textSize="@dimen/text_size_16" />

                        <TextView
                            android:id="@+id/fim_total_score"
